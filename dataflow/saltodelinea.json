{
	"name": "saltodelinea",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AP",
						"type": "DatasetReference"
					},
					"name": "AP"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "APDM",
						"type": "DatasetReference"
					},
					"name": "APMD"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {ID de usuario} as string,",
				"          {Usuario activo} as string,",
				"          Apellidos as string,",
				"          Nombre as string,",
				"          {Segundo nombre} as string,",
				"          {ID de entidad} as string,",
				"          {Tipo de item de aprendizaje} as string,",
				"          {Tipo de entidad} as string,",
				"          {Fecha de revisión} as string,",
				"          {Número de revisión} as string,",
				"          {Título de entidad} as string,",
				"          {Fecha de vencimiento} as string,",
				"          {Días rest.} as string,",
				"          {Zona horaria preferida} as string,",
				"          {Tipo de asignación} as string,",
				"          {ID de programa de estudios} as string,",
				"          {Fecha de finalización} as string,",
				"          {Fecha de suspenso} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     skipLines: 1) ~> AP",
				"AP derive({Título de entidad} = replace({Título de entidad}, \"\\n\", \"\")) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> APMD"
			]
		}
	}
}